{
  "version": 2,
  "builds": [
    {
      "src": "api/encrypt.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/decrypt.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/encrypt",
      "dest": "api/encrypt.py"
    },
    {
      "src": "/api/decrypt",
      "dest": "api/decrypt.py"
    }
  ]
}
